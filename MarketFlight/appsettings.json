{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "Monitoring": {
    "HandleAspNetLogs": true,
    "RootLogPath": "Logs",
    "GrandOutput": { // Configuration by handler (e.g. TextFileConfiguration -> TextFile)
      "Handlers": {
        "Console": true,
        "TextFile": {
          "Path": "Text"
        }
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "dbConnection": "Server=.;Database=MarketFlight;Trusted_Connection=True;"
  }
}
